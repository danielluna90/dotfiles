{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:darkBlue",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "type": "time",
          "properties": {
            "time_format": "3:04:05 PM"
          }
        },
        {
          "background": "#9787b3",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "#ffffff",
          "properties": {
            "display_host": false
          },
          "style": "diamond",
          "template": "{{if .Root}} \uf0e7 {{.UserName}} {{else}} {{.UserName}} {{end}}",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "p:purple",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:green",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "template": "<,p:purple> {{ .AbsolutePWD }} </><p:purple,transparent>\ue0b0</> ",
    "background": "transparent",
    "foreground": "#ffffff",
    "Text": ""
  },
  "palette": {
    "blue": "#61AFEF",
    "purple": "#e0a3f3",
    "green": "#95ffa4",
    "darkBlue": "#003f50"
  },
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.UserName}} \u2794 üìÅ{{.Folder}}",
  "final_space": true,
  "version": 2
}